    <config key="metadata">
        <entry key="description" type="xstring" value="This component formats data before analyzing recurring events, such as monthly or annually recurring revenue. In the input table, each record holds a time period, an ID, and some numeric value. These records could be, for example, contracts with different customers. In the formatted output table, each record holds a time point within the time period, ID, and value. This value is calculated by dividing the original value by the number of single time points within the time period.  %%00013%%00010%%00013%%00010The granularity of the single time points can be set to &quot;Days&quot; or &quot;Months&quot;.  %%00013%%00010%%00013%%00010Required extensions:%%00013%%00010- KNIME Math Expression (JEP) %%00013%%00010(https://hub.knime.com/knime/extensions/org.knime.features.ext.jep/latest)%%00013%%00010- KNIME Quick Forms %%00013%%00010(https://hub.knime.com/knime/extensions/org.knime.features.js.quickforms/latest)"/>
        <entry key="type" type="xstring" value="VISUALIZER"/>
        <entry key="icon" type="xstring" value=""/>
        <config key="inports">
            <config key="inport_0">
                <entry key="name" type="xstring" value="Contracts data "/>
                <entry key="description" type="xstring" value="- Two columns of type Date&amp;Time that define the start and end date of the time period %%00013%%00010- Numeric or string column for the IDs that identify each record%%00013%%00010- Numeric column for the values that are assigned to each time period and ID%%00013%%00010"/>
                <entry key="index" type="xint" value="0"/>
            </config>
        </config>
        <config key="outports">
            <config key="outport_0">
                <entry key="name" type="xstring" value="Recurring values"/>
                <entry key="description" type="xstring" value="- Column of type Date&amp;Time for the single time points that have been extracted from each time period%%00013%%00010- String or numeric column for the IDs of the records%%00013%%00010- Numeric column for the values assigned to each time point and ID"/>
                <entry key="index" type="xint" value="0"/>
            </config>
        </config>
    </config>
